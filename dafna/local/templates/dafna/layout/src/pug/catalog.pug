              +SelectBoxCh('radio-material', 'Материал', [
                {name: 'Серебро (238)', value: 'r1', checked: true},
                {name: 'Ювелирный сплав (230)', value: 'r2'},
                {name: 'Натуральные камни (34)', value: 'r3'},
                {name: 'Фарфор (89)', value: 'r4'},
                {name: 'Стекло (1)', value: 'r5'}
              ])
            .filters-line__item
              +SelectBoxCh('radio-covering', 'Покрытие', [
                {name: 'Серебро (238)', value: 'r6', checked: true},
                {name: 'Ювелирный сплав (230)', value: 'r7'},
                {name: 'Натуральные камни (34)', value: 'r8'},
                {name: 'Фарфор (89)', value: 'r9'},
                {name: 'Стекло (1)', value: 'r10'}
              ])
            .filters-line__item
              +SelectBoxCh('radio-insert', 'Вставка', [
                {name: 'Серебро (238)', value: 'r11', checked: true},
                {name: 'Ювелирный сплав (230)', value: 'r12'},
                {name: 'Натуральные камни (34)', value: 'r13'},
                {name: 'Фарфор (89)', value: 'r14'},
                {name: 'Стекло (1)', value: 'r15'}
