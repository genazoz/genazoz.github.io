extends ./templates/_layout

block variables
  -
    var appId = 'Catalog';
    var title = 'Catalog';
    var description = 'Description: This page is catalog page.';

block content
  include layouts/_header.pug
  include layouts/_header-m.pug
  include components/custom-select/_custom-select.pug

  .M.M-filters-mbl(data-toggle-modal-btn='' data-mw_scrolled='')
    .M__cover
    .M__wrapper(data-mw_scrolled='')
      form.M__main-wrapper.M-filters-mbl__main-wrapper(action="" data-modal-wrapper='')
        .M__header.M-filters-mbl__header
          p.M-filters-mbl__header-title Фильтры
          .M-filters-mbl__close-btn(data-toggle-modal-btn='')
            svg(xmlns='http://www.w3.org/2000/svg' width='15' height='15' viewbox='0 0 15 15' fill='none')
              path(d='M1 1L13.7279 13.7279' stroke='#222222' stroke-linecap='round' stroke-linejoin='round')
              path(d='M13.7285 1L1.00059 13.7279' stroke='#222222' stroke-linecap='round' stroke-linejoin='round')
        .M__content.M-filters-mbl__content
          .M-filters-mbl__content-form
            .M-filters-mbl__content-title Цена
            .range-slider
              .range-slider__result
                .range-slider__result-item.range-slider__result-item-from
                  input.js-input-from(type='number' value='0' min='0' max='150000' step='1')
                .range-slider__result-item.range-slider__result-item-to
                  input.js-input-to(type='number' value='150000' min='0' max='150000' step='1')
              .range-slider__item
              //- (range-min='0' range-max='150000' range-step='1' range-min-value='0' range-max-value='150000')
          .M-filters-mbl__content-form
            .M-filters-mbl__content-title Тип украшения
            .M-filters-mbl__content-checkboxes-wrapper
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch1")
                label(for="ch1")  Пусеты (238)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch2")
                label(for="ch2") Серьги-кольца (230)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch3")
                label(for="ch3") На цепочке (89)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch4")
                label(for="ch4") На английском замке (34)
          .M-filters-mbl__content-form
            .M-filters-mbl__content-title Материал
            .M-filters-mbl__content-checkboxes-wrapper
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch5")
                label(for="ch5") Золото (238)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch6")
                label(for="ch6") Серебро (230)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch7")
                label(for="ch7") Камень (89)
              .custom-checkbox-a.M-filters-mbl__checkbox
                input(type="checkbox" id="ch8")
                label(for="ch8") Фарфор (34)
        .M__footer.M-filters-mbl__footer
          button Применить
  // Modal Window filters goods

  main.main(data-scroll-section='')
    section.s-catalog.s--top(data-header-mode="dark-mode")
      .container.s-catalog__container
        h1.section-title.s-catalog__title Каталог
        .filter-buttons-m.s-catalog__filter-buttons-m
          button.filter-buttons-m__item.button-c(data-toggle-modal-btn data-switching-modal="M-filters-mbl") Фильтры
          .filter-buttons-m__item
            +SelectBoxRd('radio-filters-mbl2', 'Популярные', [
              {name: 'Популярные', value: 'r33', checked: true},
              {name: 'Дорогие', value: 'r34'},
              {name: 'Дешевые', value: 'r35'},
              {name: 'Акции', value: 'r36'},
              {name: 'Новинки', value: 'r37'}
            ])
        .s-catalog__categories
          a.s-catalog__categories-item(href="javascript:void(0)") Коллекция Dafna
          a.s-catalog__categories-item(href="javascript:void(0)") Серьги
          a.s-catalog__categories-item(href="javascript:void(0)") Кольца
          a.s-catalog__categories-item(href="javascript:void(0)") Каффы
          a.s-catalog__categories-item(href="javascript:void(0)") Украшения на шею
          a.s-catalog__categories-item(href="javascript:void(0)") Браслеты
          a.s-catalog__categories-item(href="javascript:void(0)") Броши
        .s-catalog__fixed-scroll
          .filters-line(data-scroll, data-scroll-sticky, data-scroll-target=".s-catalog__fixed-scroll", data-scroll-class="filters-line--fixed", data-scroll-repeat)
